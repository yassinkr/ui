{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next" 
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1" 
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://it-company-back.vercel.app/api" 
    }
  }
  